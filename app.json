{
  "expo": {
    "name": "Todo Mobile",
    "slug": "todo-mobile",
    "version": "0.1.0",
    "orientation": "portrait",
    "scheme": "todo-mobile",
    "userInterfaceStyle": "dark",
    "ios": {
      "bundleIdentifier": "com.todomd.mobile",
      "supportsTablet": false,
      "infoPlist": {
        "NSUserNotificationUsageDescription": "Todo Mobile sends reminders for upcoming tasks."
      }
    },
    "platforms": ["ios"],
    "plugins": ["expo-notifications"]
  }
}
